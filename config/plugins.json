{
  "assetOptions": {
    "source": "assets/",
    "destination": "assets/"
  },
  "babelOptions": {
    "presets": ["es2015"]
  },
  "pugOptions": {
    "pretty": false,
    "partials": "partials",
    "rename": true
  },
  "sassOptions": {
    "outputStyle": "compressed",
    "precision": 10
  },
  "postcssOptions": {
    "plugins": {
      "autoprefixer": {
        "browsers": ["last 3 versions"]
      },
      "cssnano": {
        "autoprefixer": false,
        "discardComments": {
          "removeAll": true
        }
      },
      "postcss-import": {}
    }
  },
  "bsOptions": {
    "server": "build",
    "files": [
      "src/**/*.{md,html,scss}",
      "layouts/**/*.{html,pug}",
      "partials/**/*.{html,pug"
    ]
  },
  "mardownOptions": {},
  "dataMarkdownOptions": {
    "removeAttributeAfterwards": true
  },
  "permalinkOptions": {},
  "htmlminOptions": {}
}